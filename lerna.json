{
  "version": "0.0.3-alpha.0",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "registry": "https://github.com/qiupo/",
    "publish": {
      "allowBranch": "master"
    }
  },
  "ignoreChanges": [
    "**/*.md",
    "**/*.test.ts",
    "**/*.e2e.ts",
    "**/fixtures/**",
    "**/test/**"
  ],
  "packages": [
    "packages/*"
  ]
}